<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Agent_CSAT_Rating_Score__c</fullName>
    <externalId>false</externalId>
    <formula>CASE(Agent_CSAT_Rating__c, 
					&quot;Very Good&quot;, 4,
					&quot;Good&quot;, 3,
					&quot;Neutral&quot;, 2,
					&quot;Bad&quot;, 1,
					&quot;Very Bad&quot;, 0, null)</formula>
    <inlineHelpText>Calculates Score based on Agent CSAT Rating for Member Care Closed Case Surveys, where Very Good = 4, Good = 3, Neutral = 2, Bad = 1, and Very Bad = 0.</inlineHelpText>
    <label>Agent CSAT Rating Score</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
