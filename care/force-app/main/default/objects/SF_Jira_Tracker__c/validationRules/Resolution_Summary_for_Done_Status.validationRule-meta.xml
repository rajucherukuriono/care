<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Resolution_Summary_for_Done_Status</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
  ISPICKVAL( Jira_Status__c , &quot;Done&quot;),
  ISBLANK(Jira_Resolution_Summary__c)
)</errorConditionFormula>
    <errorMessage>Please Add Jira Resolution Summary</errorMessage>
</ValidationRule>
