    <rules>
        <fullName>Assigned Task to Lead</fullName>
        <actions>
            <name>Set_to_Pending</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>update_Contact_Cycle_Day1</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Remember_to_Call_Send_Introductory_Email</name>
            <type>Task</type>
        </actions>
        <active>false</active>
        <description>Vaibhav Kulkarni (10/19/2011) If Lead status is pending then fire immediate as well as time dependent workflow</description>
        <formula>OR(AND( $RecordType.DeveloperName = &apos;SCR_Seeker&apos;, ISPICKVAL(Status, &apos;Pending&apos;)), AND( $RecordType.DeveloperName = &apos;SCR_Seeker&apos;, ISPICKVAL(Status, &apos;Unassigned&apos;), OwnerId = $User.Id ))</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>update_Contact_Cycle_Day2</name>
                <type>FieldUpdate</type>
            </actions>
            <actions>
                <name>Call_Followup_Email</name>
                <type>Task</type>
            </actions>
            <timeLength>1</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <actions>
                <name>update_Contact_Cycle_Day5</name>
                <type>FieldUpdate</type>
            </actions>
            <actions>
                <name>Call</name>
                <type>Task</type>
            </actions>
            <timeLength>4</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <actions>
                <name>update_Contact_Cycle_Day8</name>
                <type>FieldUpdate</type>
            </actions>
            <actions>
                <name>Follow_up_Call</name>
                <type>Task</type>
            </actions>
            <timeLength>7</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
