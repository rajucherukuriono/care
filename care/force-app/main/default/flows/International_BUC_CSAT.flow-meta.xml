    <apiVersion>58.0</apiVersion>
    <decisions>
        <description>This will look at the Contact record&apos;s value on the field Last_CSAT_Sent_Date__c.  If it has been longer than 7 days, we can send a survey.  If it hasn&apos;t, we will not send anything.</description>
        <name>Has_it_been_at_least_a_week_since_last_CSAT</name>
        <label>Has it been at least a week since last CSAT?</label>
        <locationX>2640</locationX>
        <locationY>276</locationY>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>LastCSATGreaterThan7Days</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Contact.Last_CSAT_Sent_Date__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Record_Type_Decision</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
