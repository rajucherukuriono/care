            <conditions>
                <leftValueReference>$Record.Lost_Reasons__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Duplicate/Child Account</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Lost_Reasons__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Language Barrier</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Lost_Reasons__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Bad Contact Info</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Lost_Reasons__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Safety Issue/Violation</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Lost_Reasons__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Religious Exemption</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Lost_Reasons__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Public School</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Lost_Reasons__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Head Start</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Lost_Reasons__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Non Profit</stringValue>
                </rightValue>
