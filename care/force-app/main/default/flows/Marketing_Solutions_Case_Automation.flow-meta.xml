    <formulas>
        <description>Formula calculating the Case Type based on the Opportunity Composer Service Vertical</description>
        <name>CaseTypeFormula</name>
        <dataType>String</dataType>
        <expression>CASE({!$Record.Composer_Service__r.Vertical__c}, 
&quot;Childcare Flat Rate&quot;, &quot;Childcare Flat Rate&quot;, 
&quot;Recruiting Solutions&quot;, &quot;Recruiting Solutions&quot;,
&quot;Childcare&quot;, CASE({!$Record.Composer_Service__r.License_Level__c},
&quot;Centers&quot;, &quot;Centers&quot;,
&quot;FCCs&quot;, &quot;FCCs&quot;,
&quot;Camps &amp; Activities&quot;, &quot;Camps &amp; Activities&quot;,
&quot;Marketing Solutions&quot;),
&quot;Marketing Solutions&quot;)</expression>
    </formulas>
    <formulas>
        <name>FCCVettingSubject</name>
        <dataType>String</dataType>
        <expression>&quot;FCC Vetting &quot; + {!$Record.Account.Name}</expression>
    </formulas>
